   "source": [
    "from math import comb\n",
    "\n",
    "def prob_aabb(k, N):\n",
    "    n = 2**k\n",
    "    p = 1/4\n",
    "    q = 3/4\n",
    "    prob = 0\n",
    "    for i in range(N,n+1):\n",
    "        prob += comb(n,i)*p**i*q**(n-i)\n",
    "    return prob\n",
    "\n",
    "\n",
    "\n",
    "        \n"
   ]
  },
